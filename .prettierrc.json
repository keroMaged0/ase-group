{
  "tabWidth": 2,
  "printWidth": 100,
  "useTabs": false,
  "singleQuote": true,
  "semi": true,
  "bracketSpacing": true,
  "trailingComma": "all",
  "arrowParens": "always",
  "importOrder": ["^builtin", "^external", "^internal", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
